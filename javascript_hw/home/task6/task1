var obj = {
    className: 'open menu'
};


function removeClass(obj, cls) {


    var arr = obj.className.split(' ');


    for (var i = 0; i <arr.length; i++) {

        if (arr[i] == cls) {
            arr.shift();

        }
    }

    obj.className = arr.join(' ');

}

removeClass(obj, 'open'); // obj.className='menu'
removeClass(obj, 'blabla'); // без изменений

console.log(obj);